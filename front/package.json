    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.3.0",
    "redux-thunk": "^2.4.2",
    "reselect": "^5.1.1",
    "sass": "^1.81.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
